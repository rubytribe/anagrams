<div class="divsignup">
<div class="diverror"><%= flash[:danger] %></div><br>
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email, class: "textcss" %><br>
  <%= f.email_field :email, class: "dap_text_box form-control" %><br>
  <%= f.label :password, class: "textcss" %><br>
  <%= f.password_field :password, class: "dap_text_box form-control" %><br>
  <%= f.submit "Log in" ,class: "btnlogin"%>
<% end %>
<br>
<p class="textcss3">New user? <%= link_to "Sign up now!", signup_path %></p>
</div>
